pipeline {
    agent any

    stages {
        stage('git checkout') {
            steps {
                checkout(
                    [$class: 'GitSCM',
                     branches: [[name: "main"]],
                     userRemoteConfigs: [[url: "https://github.com/scholarbunny09/Java_app_3.0.git"]]]
                )
            }
        }
    }
}